package Bean;

import javax.faces.bean.ManagedBean;

@ManagedBean
public class User {

private String name ="BOB";



public User() {

}

public User(String name) {
	super();
	this.name = name;
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

@Override
public String toString() {
	return "User [name=" + name + "]";
}






	
	
}
